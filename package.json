{
  "name": "vite-plugin-unused-code",
  "version": "0.1.3",
  "description": "Vite/Rollup plugin to detect unused files and unused exports in used files",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --fix './**/*.{js,ts}'",
    "prepack": "unbuild",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/vite-plugin-unused-code.git"
  },
  "keywords": [
    "vite",
    "rollup",
    "deadcode",
    "unused"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CyanSalt/vite-plugin-unused-code/issues"
  },
  "homepage": "https://github.com/CyanSalt/vite-plugin-unused-code#readme",
  "dependencies": {
    "fast-glob": "^3.3.2",
    "micromatch": "^4.0.8"
  },
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.3.0",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^9.0.4",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "release-it": "^17.11.0",
    "rollup": "^4.29.1",
    "typescript": "^5.7.2",
    "unbuild": "^3.0.1",
    "vite": "^6.0.6"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  }
}
